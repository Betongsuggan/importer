<project name="Norconex Importer"
  xmlns="http://maven.apache.org/DECORATION/1.3.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">


  <skin>
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.1.0</version>
  </skin>
  
  <custom>
    <reflowSkin>
      <protocolRelativeURLs>false</protocolRelativeURLs>
      <highlightJs>false</highlightJs>
      <theme>site</theme>
      <localResources>true</localResources>
      <brand>
        <name><![CDATA[
          <span class="color-highlight">Norconex</span> Importer
          ]]>
        </name>
        <href>http://www.norconex.com/product/committer/index.html</href>
      </brand>
      <slogan><![CDATA[
          Open-Source document text extractor and transformer
          ]]>
      </slogan>
      <titleTemplate>%2$s | %1$s</titleTemplate>
      <topNav>Documentation|Contribute</topNav>
      <bottomNav>
        <column>Main</column>
        <column>Maven Documentation</column>
        <column>Norconex Products</column>
      </bottomNav>
      <bottomDescription><![CDATA[
          <a href="http://www.norconex.com"><img src="./images/norconex_logo.png"></a><br />
          Norconex Committer is released and actively maintained by 
          <a href="http://www.norconex.com">Norconex</a>. 
          ]]>
      </bottomDescription>
      <skinAttribution>false</skinAttribution>

      <breadcrumbs>false</breadcrumbs>
      <markPageHeader>false</markPageHeader>
      <toc>none</toc>
      <smoothScroll>true</smoothScroll>

      <pages>

        <index>
          <breadcrumbs>false</breadcrumbs>
          <sections>
            <body />
            <columns>2</columns>
          </sections>
        </index>

        <download>
          <breadcrumbs>false</breadcrumbs>
          <sections>
            <columns>2</columns>
          </sections>
        </download>

        <contribute>
          <sections>
            <columns>2</columns>
          </sections>
        </contribute>

      </pages>
      
      <endContent>
        <script src="$resourcePath/js/highlight.min.js"></script>
      </endContent>
    </reflowSkin>
  </custom>
  
  <bannerLeft>
    <name><![CDATA[
      <span class="color-highlight">Norconex</span> Importer
      ]]>
    </name>
    <href>http://www.norconex.com/product/importer/index.html</href>
  </bannerLeft>
  <!-- 
  <bannerRight>
    <src>images/come up with logo</src>
  </bannerRight>
   -->
  <version position="none "/>
  <publishDate position="navigation-bottom"/>
  
  <body>
    <head>
       <link rel="stylesheet" href="$resourcePath/css/highlight.min.css" />
    </head>
    <links>
      <item name="Download" href="download.html"/>
      <item name="Support" href="support.html"/>
    </links>

    <menu name="Documentation">
      <item name="Usage" href="usage.html"/>
      <item name="Configuration" href="configuration.html"/>
      <item name="Javadoc (Latest release)" href="apidocs/index.html"/>
      <item name="Release Notes" href="changes-report.html" />
    </menu>

    <menu name="Contribute">
      <item name="Issues" href="https://github.com/norconex/importer/issues"/>
      <item name="Fork on GitHub"  href="https://github.com/Norconex/importer"/>
    </menu>

    <menu name="Main">
      <item name="Support" href="support.html"/>
      <item name="Licensing" href="license.html"/>
      <item name="Norconex Site"  href="http://www.norconex.com"/>
    </menu>

    <menu name="Maven Documentation" ref="reports"/>

    <menu name="Norconex Products">
      <item name="Search Analytics" href="http://www.norconex.com/analytics"/>
      <item name="HTTP Collector" href="http://www.norconex.com/product/collector-http"/>
      <item name="Committer"  href="http://www.norconex.com/product/committer"/>
      <item name="JEF" href="http://www.norconex.com/product/jef"/>
      <item name="Commons Lang" href="http://www.norconex.com/product/commons-lang"/>
    </menu>


  </body>

  <googleAnalyticsAccountId>UA-23162620-1</googleAnalyticsAccountId>
</project>
